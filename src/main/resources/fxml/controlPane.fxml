<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.HBox?>
<HBox xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="pl.player.mp3player.controller.ControlPaneController">
    <children>
        <Button fx:id="previousButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Button"
                HBox.hgrow="ALWAYS"/>
        <ToggleButton fx:id="playButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="ToggleButton"
                      HBox.hgrow="ALWAYS"/>
        <Button fx:id="nextButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Button"
                HBox.hgrow="ALWAYS"/>
        <Slider fx:id="volumeSlider" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS"/>
        <Slider fx:id="progressSlider" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS"/>
    </children>
</HBox>
